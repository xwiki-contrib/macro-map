  <content>{{box cssClass="floatinginfobox" title="**Contents**"}}
{{toc/}}
{{/box}}

= Description =

{{velocity}}
#set($macro = $doc.getObject('XWiki.WikiMacroClass'))
$doc.display("description", $macro)
#if($macro.getProperty("supportsInlineMode").value == 0)

{{box}}This macro does not supports inline mode.{{/box}}
#end
{{/velocity}}

= Usage =

== Content ==

{{velocity}}
#if("$!macro.getProperty('contentType').value" == "Mandatory")
{{info}}Content is mandatory{{/info}}

$doc.display("contentDescription")
#elseif("$!macro.getProperty('contentType').value" == "Optional")
{{info}}Content is optional{{/info}}

$doc.display("contentDescription")
#else
No content.
#end
#set($params = $doc.getObjects("XWiki.WikiMacroParameterClass"))
#if($params.size() &gt; 0)

== Parameters ==
|=Name|=Mandatory|=Description
#foreach($param in $params)
|**$doc.display("name", $param)**|$doc.display("mandatory", $param)|$doc.display("description", $param)
#end
#end

= Exemples =

== Simple map with default OpenStreetMap tiles==

{{code}}
{{map location="35 rue Beaubourg, 75003 Paris" width="50%" /}}

{{map location="47.156427, 27.576387" width="50%" locationType="latlng" /}}
{{/code}}

{{map location="35 rue Beaubourg, 75003 Paris" width="50%" /}}

----

{{map location="47.156427, 27.576387"  width="50%" locationType="latlng" /}}

== Simple map with custom OpenStreetMap tiles==

{{code}}
{{map location="35 rue Beaubourg, 75003 Paris" tiles="http://{s}.tile.stamen.com/watercolor/{z}/{x}/{y}.jpg" /}}
{{/code}}

{{map location="35 rue Beaubourg, 75003 Paris" tiles="http://{s}.tile.stamen.com/watercolor/{z}/{x}/{y}.jpg" /}}

== Simple map with address geocoding and Google Maps tiles==

{{code}}
{{map location="35 rue Beaubourg, 75003 Paris" library="googlemaps" /}}
{{/code}}

{{map location="35 rue Beaubourg, 75003 Paris"  library="googlemaps" /}}

== Map with parameters ==

{{code}}
{{map location="Rue des Thermopyles, 75014 Paris" width="400px" height="300px" library="googlemaps"}}
A beautiful street in Paris
{{/map}}
{{/code}}

{{map location="Rue des Thermopyles, 75014 Paris" width="400px" height="300px" library="googlemaps"}}
A beautiful street in Paris
{{/map}}


{{/velocity}}
</content>
